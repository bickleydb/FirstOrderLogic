<World>
	<constant name="Homer" domain="person"></constant>
	<constant name="Bob" domain="person"></constant>
	<constant name="James" domain="person"></constant>
	<constant name="Phil" domain="person"></constant>
	<constant name="Dude" domain="person"></constant>
	<constant name="SomeGuy" domain="person"></constant>
	<constant name="John Jacob Jingerheimerschmitt" domain="person"></constant>

	<function name="Drinks With" args = "2" domain = "Drinking">
